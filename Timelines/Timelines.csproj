<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <AssemblyName>DevInstance.Timelines</AssemblyName>
    <RootNamespace>DevInstance.Timelines</RootNamespace>
    <Authors>Yuriy Frankiv</Authors>
    <Company>DevInstance</Company>
    <Product>Timelines</Product>
    <Copyright>Copyright 2021 DevInstance</Copyright>
    <AssemblyVersion>0.0.0.1</AssemblyVersion>
    <FileVersion>0.0.0.1</FileVersion>
    <Version>0.0.1</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="compilerconfig.json" />
    <Content Remove="packages.config" />
    <Content Remove="wwwroot\Timeline.razor.min.css" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="styles\default\bar-color.scss" />
    <None Remove="styles\default\circle-color.scss" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="styles\default\circle-color.scss" />
    <Content Include="styles\default\bar-color.scss" />
    <Content Include="styles\default\color.scss" />
  </ItemGroup>

  <ItemGroup>
    <None Include="compilerconfig.json" />
    <None Include="packages.config" />
  </ItemGroup>

  
  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="DevInstance.LogScope" Version="1.0.0" />
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="5.0.3" />
  </ItemGroup>

</Project>
